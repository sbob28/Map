{
  "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
  "title": "Migration Reasons of Birds",
  "data": {"url": "bird_migration_data.csv"},
  "transform": [{"aggregate": [{"op": "count", "as": "Count"}], "groupby": ["Migration_Reason"]}],
  "mark": {"type": "arc", "innerRadius": 70},
  "encoding": {
    "theta": {"field": "Count", "type": "quantitative"},
    "color": {"field": "Migration_Reason", "type": "nominal", "title": "Migration Reason"},
    "tooltip": [{"field": "Migration_Reason"}, {"field": "Count"}]
  },
  "view": {"stroke": null}
}
